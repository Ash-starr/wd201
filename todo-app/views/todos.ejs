<h5>
   <%= title %> <span
         class="bg-gray-100 text-gray-900 text-sm font-small mr-2 px-2.5 py-0.5 rounded dark:bg-gray-200 dark:text-gray-900 justify-center items-center items">
         <%= data.length %>
      </span>
</h5>
<ul class="list-none">
   <% for(let i=0;i<data.length;i++) { %>
      <li class="Todo-Item">
         <div class="flex items-center w-fit my-2 px-2 py-1 rounded hover:bg-purple-50">
            <input type="checkbox" id="todo-checkbox-<%= data[i].id %>" <%=data[i].completed ? "checked" :"" %>
            onclick="updateTodo(<%= data[i].id %>)"
               class="w-4 h-4 text-blue-600 bg-gray-100">
               <label for="todo-checkbox-<%= data[i].id %>" class="ml-2 text-sm text-gray-600 cursor-pointer">
                  <%= data[i].title%>
               </label>
               <a href="#" class="hidden  trash-icon ml-2" onclick="deleteTodo(<%= data[i].id %> )">
                  <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none"
                     stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
                     class="feather feather-trash">
                     <polyline points="3 6 5 6 21 6"></polyline>
                     <path d="M19 6v14a2 2 0 0 1-2 2H7a2 2 0 0 1-2-2V6m3 0V4a2 2 0 0 1 2-2h4a2 2 0 0 1 2 2v2"></path>
                  </svg>
               </a>
         </div>
      </li>
      <% } %>
</ul>